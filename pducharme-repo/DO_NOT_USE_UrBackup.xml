<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>UrBackup</Name>
  <Description>
    Full and incremental image and file backups for computers.[br]
    [b]/backup[/b] : this path is used to store the images files of computers.[br]
 </Description>
  <Registry>https://registry.hub.docker.com/u/pducharme/urbackupserver/</Registry>
  <Repository>pducharme/urbackupserver</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>55413</HostPort>
        <ContainerPort>55413</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>55414</HostPort>
        <ContainerPort>55414</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>55415</HostPort>
        <ContainerPort>55415</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>35623</HostPort>
        <ContainerPort>35623</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>      
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/</HostDir>
      <ContainerDir>/backup</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/</HostDir>
      <ContainerDir>/tmp</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:55414]/</WebUI>
  <Banner>http://i.imgur.com/QOY5dBr.png</Banner>
  <Icon>http://i.imgur.com/QOY5dBr.png</Icon>
</Container>

